SELECT  
    F.trx_code COD,
    F.resort CIA,
    F.trx_date FECHA,
    F.ORIGINAL_RESV_NAME_ID RESERV,
    CASE WHEN N.ID_TYPE IS NULL THEN '2' ELSE N.ID_TYPE END AS TIP,
    CASE WHEN N.TAX1_NO IS NULL THEN '222222223' ELSE N.TAX1_NO END AS TERCERO,
    N.NAME_KEYWORDS SUC,
    N.NATIONALITY NAC,
    REPLACE(F.NET_AMOUNT, '.', ',') AS VALOR,
    F.ROOM HAB,
    N.NAME_KEYWORDS RECIBO,
    F.TRX_NO,
    F.BILL_NO FACTURA,
    F.REFERENCE REFERENCIA,
    F.FOLIO_TYPE NOTAS,
    F.RECPT_TYPE NRO_TARJETA,
    F.RECPT_TYPE AUTORIZACION,
    F.RECPT_TYPE FECHA_DE_VENC,
    F.RECPT_TYPE FACT_ASOC,
    F.RECPT_TYPE TIPO_DOC,
    REPLACE(F.NET_AMOUNT, '.', ',') AS VALOR_ME,
    N.FIRST NOMBRES,
    N.SNAME APELLIDOS,
    N.ADDRESS1 ADDRESS1,
    N.CITY CITY,
    N.COUNTRY COUNTRY,
    N.PHONE_NO PHONE_NO,
    N.EMAIL EMAIL,
    '1102' CODE_POSTAL
    
FROM  
    FINANCIAL_TRANSACTIONS_E F
INNER JOIN 
    NAME_VIEW N 
    ON F.NAME_ID = N.NAME_ID
WHERE  
    F.NET_AMOUNT <> '0' 
    AND F.trx_date = '03-DEC-2024'