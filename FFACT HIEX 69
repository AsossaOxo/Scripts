SELECT  
    F.FOLIO_TYPE DOC,
    F.BILL_NO CONS,
    F.TRX_DATE  F_FACTUR,
    F.TRX_DATE  F_CORTE,
    F.resort CIA,
    N.ID_TYPE TIP,
    N.TAX1_NO TERCERO,
    N.NAME_KEYWORDS SUC,
    N.NATIONALITY NAC,
    F.ORIGINAL_RESV_NAME_ID RESERV,
    F.trx_code COD,
    F.TRX_DATE  FECHA_T,
    REPLACE(F.NET_AMOUNT, '.', ',') AS VALOR,
    F.ROOM HAB,
    F.BILL_NO FACTURA,
    F.TRX_NO,
    N.NAME_KEYWORDS RECIBO,
    N.FIRST NOMBRES,
    N.SNAME APELLIDOS,
    F.EXCHANGE_RATE TASADECAMBIO,
    F.CURRENCY TIPOMONEDA,
    F.CURRENCY TIPOMONEDA,
    N.ADDRESS1 ADDRESS1,
    N.PHONE_NO PHONE_NO,
    N.CITY CITY,
    N.COUNTRY COUNTRY,
    N.EMAIL EMAIL,
    '1102' CODE_POSTAL



FROM  
    FINANCIAL_TRANSACTIONS_E F
INNER JOIN 
    NAME_VIEW N 
    ON F.NAME_ID = N.NAME_ID
WHERE  
    F.NET_AMOUNT <> '0' 
    AND F.trx_date = '03-DEC-2024' AND F.FOLIO_TYPE IS NOT NULL
    
